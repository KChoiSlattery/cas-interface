{
  "name": "cas-interface",
  "version": "0.1.0",
  "description": "A Flask app for intuitive interfacing with Sympy's algebraic solver, differentiator, and integrator.",
  "source": [
    "src/templates/index.html",
    "src/templates/solver.html"
  ],
  "scripts": {
    "postinstall": "npx patch-package"
  },
  "parcel-namer-rewrite": {
    "chain": "@parcel/namer-default",
    "hashing": "always",
    "disable": false,
    "rules": {
      "\\b(\\w+)\\.(?:js)\\b": "static/scripts/$1{.hash}.js",
      "\\b(\\w+)\\.(?:css|scss)\\b": "static/style/$1{.hash}.css",
      "\\b(\\w+)\\.(?:ttf)\\b": "static/style/fonts/$1.ttf",
      "\\b(\\w+)\\.(?:woff)\\b": "static/style/fonts/$1.woff",
      "\\b(\\w+)\\.(?:woff2)\\b": "static/style/fonts/$1.woff2",
      "\\b(\\w+)\\.(?:ico)\\b": "static/images/$1.ico",
      "\\b(\\w+)\\.(?:png)\\b": "static/images/$1.png",
      "\\b(\\w+)\\.(?:jpg|jpeg)\\b": "static/images/$1.jpg",
      "\\b(\\w+)\\.(?:html)\\b": "templates/$1.html"
    }
  },
  "targets": {
    "default": {
      "distDir": "dist"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KChoiSlattery/cas-interface.git"
  },
  "author": "Kieran Choi-Slattery",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KChoiSlattery/cas-interface/issues"
  },
  "homepage": "https://github.com/KChoiSlattery/cas-interface#readme",
  "devDependencies": {
    "@gatsbyjs/parcel-namer-relative-to-cwd": "^2.13.1",
    "@parcel/transformer-sass": "^2.12.0",
    "@types/jquery": "^3.3.29",
    "@types/katex": "^0.16.7",
    "@types/select2": "^4.0.63",
    "parcel": "^2.12.0",
    "parcel-namer-rewrite": "^2.10.3-rc.2",
    "process": "^0.11.10",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "jquery": "^3.4.1",
    "katex": "^0.16.22",
    "mathquill-typescript": "^2.0.9",
    "patch-package": "^8.0.0",
    "select2": "^4.1.0-rc.0"
  }
}
